{
    "name": "raystech/laravel-starter-kit",
    "description": "Starter Kit for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Piyapan Rodkuen",
            "email": "piyapannr@gmail.com",
            "homepage": "http://www.raystech.github.io"
        }
    ],
    "homepage": "https://github.com/raystech/laravel-starter-kit",
    "keywords": ["Laravel", "Starter", "Start", "Kit"],
    "require": {
        "php": "^7.2",
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Raystech\\StarterKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Raystech\\StarterKit\\Tests\\": "tests",
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Raystech\\StarterKit\\StarterKitServiceProvider"
            ],
            "aliases": {
                "StarterKit": "Raystech\\StarterKit\\Facades\\StarterKit"
            }
        }
    }
}